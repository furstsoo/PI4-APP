<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context="com.project.activity.MainActivity">
  <Button
          android:text="Login"
          android:layout_width="129dp"
          android:layout_height="45dp" android:id="@+id/login"
          app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="64dp"
          android:layout_marginStart="64dp" app:layout_constraintEnd_toStartOf="@+id/register"
          app:layout_constraintHorizontal_bias="0.0" app:layout_constraintBottom_toBottomOf="parent"
          android:layout_marginBottom="56dp" app:layout_constraintTop_toBottomOf="@+id/logo"
          app:layout_constraintVertical_bias="1.0"/>
  <Button
          android:text="Cadastro"
          android:layout_width="129dp"
          android:layout_height="46dp" android:id="@+id/register"
          app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
          android:layout_marginEnd="68dp" android:layout_marginRight="68dp" android:layout_marginBottom="56dp"
          app:layout_constraintTop_toTopOf="@+id/logo" android:layout_marginTop="599dp"
          app:layout_constraintVertical_bias="1.0"/>
  <ImageView
          android:layout_width="378dp"
          android:layout_height="586dp" app:srcCompat="@drawable/ic_launcher_foreground"
          android:id="@+id/logo" app:layout_constraintTop_toTopOf="parent"
          android:layout_marginTop="16dp" app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>